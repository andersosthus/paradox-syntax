{
    "name": "paradox-syntax",
    "displayName": "Paradox Syntax Highlighter",
    "description": "Syntax highlighter for the scripting language used by Paradox in their games. Syntax file from https://github.com/taw/sublime-paradox",
    "version": "1.2.0",
    "publisher": "andersosthus",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "paradox",
                "aliases": [
                    "Paradox",
                    "paradox"
                ],
                "extensions": [
                    ".txt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "paradox",
                "scopeName": "source.paradox",
                "path": "./syntaxes/paradox.tmLanguage"
            }
        ]
    }
}